@model SmallSqliteKit.Service.Models.DatabaseBackup

<td>@Model.DatabasePath</td>
<td>@Model.BackupFrequency</td>
<td>@Model.LastBackupTime</td>
<td>@(Model.UploadToDropbox ? "Yes" : "No")</td>
<td>@Model.UploadToDropboxFrequency</td>
<td>@Model.LastUploadToDropboxTime</td>
<td>@(Model.Optimize ? "Yes" : "No")</td>
<td>@Model.OptimizeFrequency</td>
<td>@Model.LastOptimizeTime</td>
<td>@(Model.Vacuum ? "Yes" : "No")</td>
<td>@Model.VacuumFrequency</td>
<td>@Model.LastVacuumTime</td>
